ent/src/App.js",[],"/Users/emilygilbertsen/Desktop/Working Class File/uofo-por-fsf-pt-07-2020-u-c/21-MERN/02-Homework/Google-Books-2/client/src/pages/Search.js",["104"],"/Users/emilygilbertsen/Desktop/Working Class File/uofo-por-fsf-pt-07-2020-u-c/21-MERN/02-Homework/Google-Books-2/client/src/pages/NoMatch.js",[],"/Users/emilygilbertsen/Desktop/Working Class File/uofo-por-fsf-pt-07-2020-u-c/21-MERN/02-Homework/Google-Books-2/client/src/pages/Shelf.js",["105"],"/Users/emilygilbertsen/Desktop/Working Class File/uofo-por-fsf-pt-07-2020-u-c/21-MERN/02-Homework/Google-Books-2/client/src/components/Nav/index.js",[],"/Users/emilygilbertsen/Desktop/Working Class File/uofo-por-fsf-pt-07-2020-u-c/21-MERN/02-Homework/Google-Books-2/client/src/utils/API.js",["106"],"/Users/emilygilbertsen/Desktop/Working Class File/uofo-por-fsf-pt-07-2020-u-c/21-MERN/02-Homework/Google-Books-2/client/src/components/SaveBtn/index.js",[],["107","108"],"/Users/emilygilbertsen/Desktop/Working Class File/uofo-por-fsf-pt-07-2020-u-c/21-MERN/02-Homework/Google-Books-2/client/src/components/Jumbotron/index.js",[],"/Users/emilygilbertsen/Desktop/Working Class File/uofo-por-fsf-pt-07-2020-u-c/21-MERN/02-Homework/Google-Books-2/client/src/components/DeleteBtn/index.js",[],"/Users/emilygilbertsen/Desktop/Working Class File/uofo-por-fsf-pt-07-2020-u-c/21-MERN/02-Homework/Google-Books-2/client/src/components/Grid/index.js",[],"/Users/emilygilbertsen/Desktop/Working Class File/uofo-por-fsf-pt-07-2020-u-c/21-MERN/02-Homework/Google-Books-2/client/src/components/List/index.js",[],"/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/index.js",[],["109","110"],"/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/App.js",[],"/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/pages/Search.js",["111"],"import React, { Component } from \"react\";\nimport SaveBtn from \"../components/SaveBtn\";\nimport API from \"../utils/API\";\nimport { Link } from \"react-router-dom\";\nimport { Col, Row, Container } from \"../components/Grid\";\nimport { List, ListItem } from \"../components/List\";\nimport Jumbotron from \"../components/Jumbotron\";\n\nclass Search extends Component {\n    state = {\n        books: [],\n        query: \"\"\n    }\n\n\n    searchGoogle = event => {\n        event.preventDefault();\n        let query = this.state.query\n        API.searchGoogle(query)\n            .then(res => \n                this.setState({\n                    books: res.data.items,\n                    query: \"\"\n                })\n            )\n            .catch(err => console.log(err));\n    };\n\n\n    handleInputChange = event => {\n        this.setState({query: event.target.value});\n    };\n\n    saveBook = bookData => {\n        API.saveBook(bookData)\n            .then(res => this.setState({books: res.data.items}))\n            .catch(err => console.log(err));\n    }\n\n    render() {\n        return (\n            <Container fluid>\n                <Row>\n                    <Col size=\"md-12\">\n                        <Jumbotron>\n                            <h1>Search Google Books</h1>\n                        </Jumbotron>\n                    </Col>\n                </Row>\n                <Row>\n                    <Col size=\"md-12\">\n                        <div className=\"form-group\">\n                            <input type=\"text\" className = \"form-control\" id=\"query\" name = \"query\" placeholder=\"search\" onChange= {this.handleInputChange} />\n                        </div>\n                        <button type = \"submit\" onClick={this.searchGoogle}>Search</button>\n                    </Col>\n                </Row>\n                <Row>\n                    <Col size=\"md-12\">\n                        <div>\n                            <h1>Results: </h1>\n                        </div>\n                        {this.state.books.length ? (\n                            <List>\n                                {this.state.books.map(book => (\n                                    <ListItem key={book._id}>\n                                        <Row>\n                                            <Col size=\"md-3\">\n                                                <img src={book.volumeInfo.imageLinks.thumbnail} />\n                                            </Col>\n                                            <Col size=\"md-9\">\n                                                <Link to={book.volumeInfo.infoLink}>\n                                                    <strong>\n                                                        {book.volumeInfo.title} by {book.volumeInfo.authors}\n                                                    </strong>\n                                                </Link>\n                                                <p>Description: {book.volumeInfo.description} </p>\n                                                <SaveBtn onClick={() => this.saveBook({\n                                                    title: book.volumeInfo.title,\n                                                    author: book.volumeInfo.authors,\n                                                    image: book.volumeInfo.imageLinks.thumbnail,\n                                                    description: book.volumeInfo.description,\n                                                    link: book.volumeInfo.infoLink\n                                                })} />\n                                            </Col>\n                                        </Row>\n                                    </ListItem>\n                                ))}\n                            </List>\n                        ) : (<div> No Results to Display </div>)}\n                    </Col>\n                </Row>\n            </Container>\n        )\n    }\n\n}\n\nexport default Search;","/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/pages/Shelf.js",["112"],"import React, { Component } from \"react\";\nimport DeleteBtn from \"../components/DeleteBtn\";\nimport API from \"../utils/API\";\nimport { Link } from \"react-router-dom\";\nimport { Col, Row, Container } from \"../components/Grid\";\nimport { List, ListItem } from \"../components/List\";\nimport Jumbotron from \"../components/Jumbotron\";\n\n\nclass Shelf extends Component {\n    state = {\n        savedBooks: []\n    };\n\n    componentDidMount() {\n        this.getBooks();\n    };\n\n    getBooks() {\n        API.getBooks()\n            .then(res =>\n                this.setState({ savedBooks: res.data })\n            )\n            .catch(err => console.log(err));\n    };\n\n    deleteBook(id) {\n        API.deleteBook(id)\n            .then(res => this.getBooks())\n            .catch(err => console.log(err));\n    };\n\n\n    render() {\n        return (\n            <Container fluid>\n                <Row>\n                    <Col size=\"md-12\">\n                        <Jumbotron>\n                            <h1>Your Bookshelf</h1>\n                        </Jumbotron>\n                        {this.state.savedBooks.length ? (\n                            <List>\n                                {this.savedBooks.map(book => (\n                                    <ListItem key={book._id}>\n                                        <Row>\n                                            <Col size=\"md-3\">\n                                                <img src={book.image} />\n                                            </Col>\n                                            <Col size=\"md-9\">\n                                                <Link to={book.link}>\n                                                    <strong>\n                                                        {book.title} by {book.author}\n                                                    </strong>\n                                                </Link>\n                                                <p>Description: {book.description} </p>\n                                                <DeleteBtn onClick={() => this.deleteBook(book._id)} />\n                                            </Col>\n                                        </Row>\n                                    </ListItem>\n                                ))}\n                            </List>\n                        ) : (\n                                <div></div>\n                            )}\n                    </Col>\n                </Row>\n            </Container>\n        );\n    }\n}\n\nexport default Shelf;","/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/pages/NoMatch.js",[],"/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/components/Nav/index.js",[],"/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/utils/API.js",["113"],"import axios from \"axios\";\n\nexport default {\n  // Gets all books\n  getBooks: function() {\n    return axios.get(\"/api/books\");\n  },\n  // Gets the book with the given id\n  getBook: function(id) {\n    return axios.get(\"/api/books/\" + id);\n  },\n  // Deletes the book with the given id\n  deleteBook: function(id) {\n    return axios.delete(\"/api/books/\" + id);\n  },\n  // Saves a book to the database\n  saveBook: function(bookData) {\n    return axios.post(\"/api/books\", bookData);\n  },\n  searchGoogle: function(query) {\n    var encodeQuery = encodeURI(query);\n    return axios.get(\"https://www.googleapis.com/books/v1/volumes?q=\" + encodeQuery);\n  }\n};","/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/components/Grid/index.js",[],"/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/components/SaveBtn/index.js",[],"/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/components/List/index.js",[],"/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/components/DeleteBtn/index.js",[],"/Users/emilygilbertsen/Desktop/GitHub/google-books-library/client/src/components/Jumbotron/index.js",[],{"ruleId":"114","severity":1,"message":"115","line":69,"column":49,"nodeType":"116","endLine":69,"endColumn":99},{"ruleId":"114","severity":1,"message":"115","line":48,"column":49,"nodeType":"116","endLine":48,"endColumn":99},{"ruleId":"117","severity":1,"message":"118","line":3,"column":1,"nodeType":"119","endLine":24,"endColumn":3},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","replacedBy":"123"},{"ruleId":"120","replacedBy":"124"},{"ruleId":"122","replacedBy":"125"},{"ruleId":"114","severity":1,"message":"115","line":69,"column":49,"nodeType":"116","endLine":69,"endColumn":99},{"ruleId":"114","severity":1,"message":"115","line":48,"column":49,"nodeType":"116","endLine":48,"endColumn":73},{"ruleId":"117","severity":1,"message":"118","line":3,"column":1,"nodeType":"119","endLine":24,"endColumn":3},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","no-native-reassign",["126"],"no-negated-in-lhs",["127"],["126"],["127"],"no-global-assign","no-unsafe-negation"]